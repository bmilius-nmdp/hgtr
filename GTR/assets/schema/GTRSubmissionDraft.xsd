										<xs:enumeration value="minimum"/>
										<xs:enumeration value="maximum"/>
										<xs:enumeration value="single"/>
									</xs:restriction>
								</xs:simpleType>
							</xs:attribute>
						</xs:extension>
					</xs:simpleContent>
				</xs:complexType>
			</xs:element>
			<xs:element name="Strain" type="xs:string" minOccurs="0"/>
